<%- include('partials/header.ejs') %>
<div class="container">
  <h2>Usuários</h2>
  <a class="btn btn-success mb-2" href="/user/register"> Criar novo usuário</a>
    <a class="btn btn-primary mb-2" href="/login">Logar</a>
        <a class="btn btn-danger mb-2" href="/logout">Sair</a>

  <table class="table table-bordered">
    <thead>
      <tr>
        <th>ID</th>
        <th>Email</th>
        <th>Nome</th>
      </tr>
    </thead>
    <tbody>
      </tr>
              <% users.forEach(user => { %>
                <tr>
                    <th><%=user.id %></th>
                    <th><%=user.email %></th>
                    <th><%=user.name %></th>           
                </tr>
           <% }) %>
    </tbody>
  </table>
</div>
<%- include('partials/footer.ejs') %>
